pipeline {
    agent any

    stages {
        stage('Hello') {
            steps {
                println(param1)
                println(param2)
            }
        }
          stage('call to DSL Job') {
            steps {
              build job: 'SeedJob', parameters: [[$class: 'StringParameterValue', name: 'param1', value: params.param1]]
            }
        }
        
    }
}
